
export const LobMatchGuard: CanMatchFn = (
  route: Route,
  segments: UrlSegment[]
) => {
  // /Mycoverage/overview
  if (segments.length >= 2 && segments[1]?.path === 'overview') {
    return true;
  }

  // /Mycoverage/:id/medical[/...]
  const id = segments[1]?.path;
  const lob = segments[2]?.path;

  if (id && lob === 'medical') {
    return true;
  }

  return false; // everything else (dental, vision) â†’ parent
};
