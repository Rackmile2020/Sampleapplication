Copy
Edit
@Injectable({ providedIn: 'root' })
export class OverviewResolver implements Resolve<any> {
  resolve(route: ActivatedRouteSnapshot) {
    return { title: 'Overview Page', showHeader: true }; // or fetch from API
  }
}
