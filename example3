const EligiblePrescription = Array.isArray(realignMyCoverageData?.subMenuItems)
  ? (() => {
      const benefits = realignMyCoverageData.subMenuItems.filter(
        (sub) => sub?.subMenuGroup === "Benefits"
      );
      return benefits.length > 0
        ? { ...realignMyCoverageData, subMenuItems: benefits }
        : null;
    })()
  : null;
